{
  "name": "gitgud",
  "author": {
    "name": "Dean Kinane"
  },
  "description": "A different type of Git client",
  "version": "0.1.10-alpha.0",
  "private": true,
  "scripts": {
    "test:unit": "vue-cli-service test:unit --testMatch **/tests/unit/**/*.spec.ts",
    "test:e2e": "vue-cli-service test:unit --config jest.e2e.config.js --testMatch **/tests/e2e/**/*.spec.ts",
    "lint": "vue-cli-service lint",
    "build": "vue-cli-service electron:build",
    "serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "preversion": "yarn test:unit",
    "postversion": "git push --tags",
    "patch": "yarn version --prepatch --preid alpha"
  },
  "main": "background.js",
  "dependencies": {
    "core-js": "^3.4.4",
    "electron-squirrel-startup": "^1.0.0",
    "node-pty-prebuilt-multiarch": "^0.9.0",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.3.0",
    "vuex": "^3.1.2",
    "xterm": "^4.3.0"
  },
  "devDependencies": {
    "@jest-runner/electron": "^2.0.3",
    "@types/jest": "^24.0.19",
    "@types/webdriverio": "4.13.0",
    "@vue/cli-plugin-babel": "^4.1.0",
    "@vue/cli-plugin-eslint": "^4.1.0",
    "@vue/cli-plugin-typescript": "^4.1.0",
    "@vue/cli-plugin-unit-jest": "^4.1.0",
    "@vue/cli-plugin-vuex": "^4.1.0",
    "@vue/cli-service": "^4.1.0",
    "@vue/eslint-config-prettier": "^5.0.0",
    "@vue/eslint-config-typescript": "^4.0.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "electron": "^5.0.0",
    "electron-builder-squirrel-windows": "^22.3.3",
    "electron-is-dev": "^1.1.0",
    "eslint": "^5.16.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-vue": "^5.0.0",
    "node-loader": "^0.6.0",
    "prettier": "^1.19.1",
    "sass": "^1.23.7",
    "sass-loader": "^8.0.0",
    "typescript": "~3.5.3",
    "vue-cli-plugin-electron-builder": "^1.4.4",
    "vue-template-compiler": "^2.6.10"
  }
}
